cmd mkdir !mkdir -p "$(echo $* | tr ' ' '\ ')"
cmd touch !touch "$(echo $* | tr ' ' '\ ')"
cmd open !~/.config/lf/scripts/open.sh "$fx"
cmd to_webp !~/.config/lf/scripts/to_webp.sh "$fx"
cmd to_zip !~/.config/lf/scripts/to_zip.sh "$fx"
cmd delete !~/.config/lf/scripts/delete.sh "$fx"
cmd bulk_rename !~/.config/lf/scripts/bulk_rename.sh "$fx"
