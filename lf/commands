cmd mkdir !mkdir -p "$(echo $* | tr ' ' '\ ')"
cmd touch !touch "$(echo $* | tr ' ' '\ ')"
cmd open !~/.config/lf/scripts/open.sh "$fx"
cmd to_webp !~/.config/lf/scripts/to_webp.py "$fx"
cmd to_zip !~/.config/lf/scripts/to_zip.py "$fx"
cmd delete !~/.config/lf/scripts/prompt_deletion.py "$fx"
cmd bulk_rename !~/.config/lf/scripts/bulk_rename.py "$fx"
cmd upscale !~/.config/lf/scripts/upscale.sh "$fx" "$1" "$2"
